[global_config]
[keybindings]
  close_term = <Alt>q
  close_window = <Alt>w
  go_down = <Primary><Shift>Down
  go_left = <Primary><Shift>Left
  go_right = <Primary><Shift>Right
  go_up = <Primary><Shift>Up
  move_tab_left = <Shift><Alt>Left
  move_tab_right = <Shift><Alt>Right
  next_tab = <Alt>Right
  prev_tab = <Alt>Left
  resize_down = <Primary><Alt>Down
  resize_left = <Primary><Alt>Left
  resize_right = <Primary><Alt>Right
  resize_up = <Primary><Alt>Up
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
[profiles]
  [[default]]
    allow_bold = False
    background_image = None
    font = DejaVu Sans Mono 11
    foreground_color = "#ffffff"
    use_system_font = False
